<div class="page-width">
  <div class="grid {{ page.handle }}">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
      </div>

      <div class="rte">
        {{ page.content }}
        {% if page.handle == 'community' %}
          {% section 'community-image-grid' %}
        {% endif %}
      </div>
    </div>
  </div>
</div>
